    "selenium",
    "automation",
    "electron"
  ],
  "author": "emrahkartals",
  "license": "PROPRIETARY",
  "dependencies": {
    "selenium-webdriver": "^4.10.0",
    "chromedriver": "^141.0.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "engines": {
    "node": ">=14.20.0"
  },
  "build": {
    "appId": "com.google.seo.bot",
    "productName": "Google SEO Bot",
    "win": {
      "target": "dir",
      "icon": "build/icon.ico"
    }
