{
  "name": "google-seo-bot",
  "version": "1.4.0",
  "description": "Google SEO Bot - An advanced Electron-based desktop application designed to improve your website's Google SEO rankings by simulating organic traffic",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
